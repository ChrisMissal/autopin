{
  "name": "autopin",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "automatically pin all tabs",
  "homepage_url": "http://github.com/ChrisMissal/autopin",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [ "js/app.js" ]
  }
}